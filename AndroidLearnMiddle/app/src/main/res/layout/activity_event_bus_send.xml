<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventBus.activities.eventBusSendActivity"
    android:orientation="vertical">


    <include layout="@layout/navigationbar" />

    <Button
        android:id="@+id/eventBus_push_sticky"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:text="发送发送消息"/>
    <Button
        android:id="@+id/eventBus_post_sticky"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:text="接受粘性事件"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="接受粘性事件内容："
        android:layout_marginTop="70dp"/>

    <TextView
        android:id="@+id/eventbus_result_sticky"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        />
</LinearLayout>